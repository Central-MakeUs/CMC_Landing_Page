{
  "name": "cmc-homepage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint:pt": "prettier --write \"${*}**/*.{ts,tsx,json}\"",
    "lint:es": "eslint --ext .ts,.tsx,.js --fix .",
    "lint": "yarn lint:pt && yarn lint:es",
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "lint-staged": {
    "*.json": [
      "prettier --cache --write"
    ],
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --cache --write"
    ]
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "next": "13.4.19",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.6.2",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "eslint": "^8.49.0",
    "eslint-config-next": "^13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "sass": "^1.67.0",
    "typescript": "^5.2.2"
  },
  "packageManager": "yarn@3.6.3"
}
